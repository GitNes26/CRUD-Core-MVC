@model List<User>
<h1>USUARIOS</h1>

<table class="table">
    <thead>
        <tr>
            <th>Usuario</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(i=>item.Username)</td>
            </tr>
        }
    </tbody>
</table>